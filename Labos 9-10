int puntoFijo(vector<int> v){
    int d = 0;
    int h = v.size();
    while(d < h){
        int medioIndice = (d+h)/2;
        int medioElem = v[medioIndice];
        if(medioElem < medioIndice){
            d = medioIndice + 1;
        } else {
            h = medioIndice;
        }
    }
    if(v[d] != d){
        return -1;
    } else {
        return d;
    }
}
